.explain {
    position: relative;
    color: #909090;
    padding-left: 10px;
    top: 10px;
    border-left: 2px solid #FF5353;
}

.mui-bar-nav ~ .mui-content {
    padding-top: 0px;
}
.mui-icon-plusempty:before{
    color: #FFFFFF;
}

#OA_task_1 .mui-table-view-cell .mui-slider-handle h4,
#OA_task_1 .mui-table-view-cell .mui-slider-handle h4 span {
    font-weight: 100;
    font-size: 16px;
    padding-bottom: 5px;
}

#OA_task_1 .mui-table-view-cell .mui-slider-handle .img {
    float: right;
}

#OA_task_1 .mui-table-view-cell .mui-slider-handle .img img {
    position: absolute;
    width: 23px;
    height: 23px;
    bottom: 18px;
    right: 0px;
}

#OA_task_1 .mui-table-view-cell .mui-slider-right img {
    width: 17px;
    height: 22px;
}

.Prompt {
    position: relative;
    top:10px;
    width: 100%;
    height:95px;
    background-color: #fff;
    padding: 0 10px;
    float: left;
    padding-top: 15px;
}

.Prompt img {
    margin-right: 10px;
    width: 15px;
    height: 15px;
}