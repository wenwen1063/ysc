.title {
    height: 120px;
    background-color: #fea79f;
    width: 100%;
    position: relative;
    padding-top: 20px;
    padding-left: 10px;
}

.title p {
    color: #FFFFFF;
    margin-bottom: 20px;
}

.title h1 {
    font-weight: 100;
    color: #FFFFFF;
}

.mingxi {
    position: relative;
    top:10px;
    background-color: #FFFFFF;
    width: 100%;
    height: 60px;
    border-bottom: 1px solid #e0e0e0;
}

.mingxi span img {
    position: relative;
    top:2px;
    width: 28px;
    height: 30px;
    margin-right: 10px;
}

.mingxi span {
    color: #4a4a4a;
    position: relative;
    top: 10px;
    padding-left: 15px;
}
.list{
    position: relative;
    top:10px;
    border-bottom: 1px solid #e0e0e0;
    padding-top: 10px;
    padding-left: 20px;
    background-color: #fff;
    height:65px;
    display: none;
}

.list h5{
    font-size: 18px;
    color: #4a4a4a;

}
.list h5 ,.list p{
    width: 61%;
    float: left;
}
.list h3{
    font-weight: 100;
    right:10px;
    position: relative;
    bottom:20px;
    float: right;
}
.backm{
    position: relative;
    margin-top: 40%;
    text-align: center;
}

.backm img{
    width: 50px;
    height:50px;
    margin-bottom: 20px;
}