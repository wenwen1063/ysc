body {
    overflow-x: hidden;
    margin-bottom: 55px;
}

.header {
    position: relative;
    background-color: #ff5353;
    height: 45px;
}


.mui-title, .mui-action-back {
    font-weight: 100;
    color: #FFFFFF;
}
li{
    font-size: 16px;
    font-weight: 100;
}
#apply{
    width: 100%;
    height: 100%;
    background-color: #FFFFFF;
    position: relative;
    top: 10px;
}
#apply .leixing{
    float: right;
}
#apply .leixing img{
    margin-left: 15px;
    margin-right: 15px;
    float: left;
    width: 15px;

}
#apply .id{
    margin-left: 10px;
    padding-top: 20px;
    padding-left: 11%;
    height: 120px;;
    width: 40%;
    border: 1px solid #e0e0e0;
    position: relative;
    top: 10px;
    float: right;
    margin-bottom: 10px;
}

#apply .id h5{
    position: relative;
    left: 15px;
}
#apply .id img{
    margin: 5px;
    width: 50px;
}

.mui-table-view-cell{
	color: #4A4A4A;
}

.mui-bar-tab input{
    width: 100%;
    height:45px;
    color: #FFFFFF;
    border: none;
    background-color: #ff3131;
}