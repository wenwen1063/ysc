<script type="text/javascript">
	var index = 0;
	$(function() {
		$('.main>div').hide();
		$('.main>div:eq(' + index + ')').show();
	});
	$('.distribution_nav ul li').click(function() {
		index = $(this).index();
		$('.distribution_nav ul li').removeClass('active');
		$(this).addClass('active');
		$('.main>div').hide();
		$('.main>div:eq(' + index + ')').show();
	});
	mui.init({
		swipeBack: false
	});
	(function($) {
		$('.mui-scroll-wrapper').scroll({
