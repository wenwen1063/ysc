.none img {
    margin-bottom: 20px;
    width: 50px;
    height: 50px;
}

.none {
    display: none;
    position: relative;
    margin-top: 50%;
    width: 100%;
    text-align: center;
}

.mui-bar img {
    position: relative;
    top: 15px;
    left: 15px;
    width: 19px;
    height: 19px;
    float: left;
}

.mui-bar p {
    position: relative;
    top: 15px;
    left: 25px;
    float: left;
}

.mui-bar input {
    position: relative;
    right: 15px;
    top: 7px;
    background-color: #fe504f;
    color: #FFFFFF;
    border: none;
    height: 35px;
    width: 100px;
    border-radius: 5px;
    float: right;
}
.content{
    margin-top: 45px;
    position: relative;
    bottom: 45px;
    padding: 0 5px;
}
.list {
    border: 2px solid #FFFFFF;
    position: relative;
    margin-top: 10px;
    width: 50%;
    height: 220px;
    float: left;
    background-color: #fff;
}

.list .img {
    width: 100%;

}

.list .img img {
    width: 100%;
    height: 170px;
}

.list .name h5, .list .name p {
    color: #4a4a4a;
    width: 51%;
    float: left;
}
.list .name{
    background-color:#FFFFFF;
}
.list .name p {
    color: #fe504f;
}

.list .name img {
    right: 10px;
    position: relative;
    bottom: 10px;
    width: 19px;
    height: 19px;
    float: right;
}















