html {
    overflow-x: hidden;
}

.header {
    position: relative;
    background-color: #ff5353;
    height: 45px;
}

.mui-title, .mui-action-back {
    font-weight: 100;
    color: #FFFFFF;
}
.mui-bar .mui-btn-link{
    color: #FFFFFF;
}
.mui-title {
    color: #000000;
    height: 35px;
}

input[type=text] {
    border: none;
}

.mui-bar .mui-title {
    color: #FFFFFF;
    background-color: #d94c4b;
    border-radius: 50px;
    width: 80%;
    left: 10px;
    height: 35px;
    position: relative;
    top: 5px;
}
.title{
    position: relative;
    width: 100%;
    height:45px;
    background-color: #FFFFFF;
}
.title h5{
    top:15px;
    font-size: 14px;
    color: #4a4a4a;
    position: relative;
    padding-left:10px;
    border-left: 2px solid #fe504f;
}
.content{
    padding-top: 20px;
    position: relative;
    width: 100%;
    height:100%;
    background-color: #fff;
}
.content .list{
    width: 90%;
    position: relative;
    left:5%;
    height:40px;

}

.content .list span{
    font-size: 18px;
    font-weight: 100;
    color: #4a4a4a;
}
.mui-icon-search:before{
    font-size: 22px;
}