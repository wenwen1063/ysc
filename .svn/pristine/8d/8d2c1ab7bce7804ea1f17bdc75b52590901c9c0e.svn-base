.mui-slider {
    position: relative;
    height: 250px;
}

.title {
    background-color: #fff;
    width: 100%;
    padding: 10px 10px;
    height: 260px;
}

.title .name {
    /*position: relative;*/
    color: #4a4a4a;
    font-size: 18px;
}

.title .xiao {
    /*position: relative;*/
    top: 5px;
    font-size: 14px;
}

.title .money {
    /*position: relative;*/
    top: 10px;
    width: 100%;
}

.title .money h5 {
    font-size: 18px;
    color: #fe504f;
    float: left;
    margin-right: 15px;
    margin-top: 0;
    margin-bottom: 0;
}

.title .money .sum .bug {
    float: left;
}

.title .money .sum {
    position: relative;
    top: 5px;
}

.title .money .sum .mui-numbox {
    position: relative;
    bottom: 5px;
    width: 120px;
    height: 28px;
}

.title .attr {
    color: #4a4a4a;
    font-size: 16px;
    padding: 5px 0;
    position: relative;
}

.title .attr .chooseattr {
    border-left: 2px solid #FF5353;
    padding-left: 10px;
}

.title .attr .rightbtn {
    position: absolute;
    right: 10px;
    width: 8px;
}

.mui-numbox [class*=btn-numbox],
.mui-numbox [class*=numbox-btn] {
    background-color: #fff;
}

.title .bao {
    padding: 5px 0;
    border-top: 1px solid #e0e0e0;
    width: 100%;
    /*position: relative;*/
    top: 20px;
    float: left;
}

.title .bao .list {
    width: 25%;
    float: left;
}

.title .bao .list p {
    font-size: 0.6rem;
    margin-bottom: 0px;
}

.title .bao img {
    position: relative;
    top: 2px;
    height: 15px;
    width: 15px;
    float: left;
    margin-right: 10px;
}

.title .you {
    padding-top: 10px;
    border-top: 1px solid #e0e0e0;
    /*position: relative;*/
    top: 10px;
    width: 100%;
    float: left;
}

.title .you .left {
    width: 60px;
    /*height: 14px;*/
    float: left;
}

.title .you .right {
    /*margin-left: 20px;*/
    float: left;
}

.title .you .right .list {
    margin: 0 10px;
    float: left;
}

.title .you .right .list h5 {
    font-size: 13px;
    color: #606060;
    font-weight: 100;
}

.title .you .right .list img {
    width: 57px;
    height: 22px;
    float: left;
}

.title .you .right .list p {
    position: relative;
    bottom: 20px;
    left: 13px;
    color: #FFFFFF;
    font-size: 12px;
}

.content {
    position: relative;
    top: 10px;
    margin-bottom: 55px;
    /*background-color: #fff;*/
}

.content .xiang {
    padding: 15px 0;
    background-color: #fff;
    display: block;
}

.content .xiang img {
    width: 100%;
    height: 100%;
    margin-left: 0;
}

.content .shoptitle {
    background-color: #FFFFFF;
    text-align: center;
    height: 45px;
    width: 100%;
    border-bottom: 1px solid #e0e0e0;
}

.content .shoptitle .list {
    margin-left: 25%;
    width: 60px;
    float: left;
    height: 45px;
}

.content .shoptitle .list h5 {
    color: #4a4a4a;
    margin: 0;
    width: 60px;
    height: 45px;
    line-height: 45px;
}

.content .shoptitle .list .red {
    width: 60px;
    padding-bottom: 15px;
    border-bottom: 1px solid #fe504f;
    color: #fe504f;
}

.shopcontent .ping {
    display: none;
    padding: 15px 0;
}

.shopcontent .ping .all {
    height: 40px;
}

.shopcontent .ping .all .list {
    float: left;
    width: 22%;
    position: relative;
    left: 5px;
    margin-left: 2%;
}

.shopcontent .ping .all .list input {
    font-weight: 100px;
    width: 100%;
    border: 1px solid #e0e0e0;
    background-color: #FFFFFF;
    color: #4a4a4a;
    border-radius: 5px;
    position: relative;
    height: 35px;
}

.shopcontent .ping .all .red input {
    background-color: #fe504f;
    color: #FFFFFF;
}

.shopcontent .ping .detailed {
    position: relative;
    top: 10px;
}

.shopcontent .ping .detailed .list {
    padding: 10px;
    background-color: #fff;
    height: 85px;
    border-bottom: 1px solid #e0e0e0;
}

.shopcontent .ping .detailed .list img {
    width: 37px;
    height: 37px;
    float: left;
}

.shopcontent .ping .detailed .list h5 {
    position: relative;
    left: 10px;
    top: 5px;
    width: 120px;
    float: left;
}

.shopcontent .ping .detailed .list a {
    position: relative;
    top: 15px;
    color: #fe504f;
    float: right;
}

.shopcontent .ping .detailed .list a span {
    color: #4a4a4a;
}

.shopcontent .ping .detailed .list p {
    position: relative;
    top: 10px;
    width: 100%;
    float: left;
}

.mui-bar p {
    position: relative;
    top: 30px;
    font-size: 12px;
    padding-top: 2px;
    float: left;
}

.mui-bar img {
    left: 21px;
    top: 10px;
    position: relative;
    float: left;
    width: 19px;
    height: 19px;
}

.mui-bar .input {
    float: right;
}

.mui-bar .input input {
    border-radius: 20px;
    background-color: #fff;
    border: 1px solid #fe504f;
    color: #fe504f;
    height: 30px;
    width: 100px;
    position: relative;
    top: 12px;
    margin-right: 15px;
}

.mui-bar .input .red {
    color: #FFFFFF;
    background-color: #fe504f;
}

#finde {
    position: fixed;
    z-index: 9999;
    bottom: 60px;
    width: 35px;
    height: 70px;
    left: 88%;
}

#finde img {
    width: 35px;
}


/*弹出层*/

.fixedbox {
    /*display: none;*/
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    text-align: center;
    z-index: 99999999;
    background: none;
    background: rgba(0, 0, 0, .5);
}

.closefun {
    width: 100%;
}

.fixedbox_body {
    position: absolute;
    left: 0;
    bottom: 0;
    padding: 25px 15px;
    display: inline-block;
    background: #fff;
    box-shadow: 0 0 10px rgba(0, 0, 0, .5);
    /*border-top: 5px solid #1EC201;*/
}

.fixedbox_body .closebtn {
    position: absolute;
    right: 15px;
    top: 20px;
    width: 0.7rem;
}
.fixedbox_body .floatattr{
    float: left;
    width: 100%;
}
.fixedbox_body .floatattr ul {
    float: left;
    width: 100%;
    padding-left: 0;
    margin: 10px 0;
    margin-bottom: 0;
}

.fixedbox_body .floatattr ul li {
    padding-bottom: 0;
    list-style: none;
    text-align: center;
    width: 50%;
    float: left;
    padding-bottom: 10px;
}
.fixedbox_body .floatattr ul li:nth-child(odd){
    padding-right: 5px;
}
.fixedbox_body .floatattr ul li:nth-child(even){
    padding-left: 5px;
}
.fixedbox_body .floatattr .spanclass {
    border: 1px solid #DFDFDF;
    display: inline-block;
    width: 100%;
    padding: 5px 0;
    color: #6B6B6B;
}

.fixedbox_body .floatattr .active .spanclass {
    background: #1279D8;
    border: 1px solid #1279D8;
    color: #FCF8FD;
}

.count img {
    vertical-align: middle;
    width: 1.5rem;
}

.count input[type='text'] {
    width: 50px;
    height: 1.75rem;
    padding: 0 5px;
}
.fixedbox_body  .price{
    color: #FE504F;
    font-size: 18px;
}
.fixedbox_body h5{
    color: #6E6E6E;
}
